{
  "name": "moul",
  "version": "3.0.0-beta.0",
  "description": "A publishing tool for photographers, visual storytellers.",
  "scripts": {
    "css": "parcel build --no-source-maps --no-content-hash assets/moul.css -d cmd/assets",
    "js": "parcel build --no-source-maps --no-content-hash assets/moul.js -d cmd/assets",
    "build": "npm run css && npm run js && cp assets/moul.toml cmd/assets"
  },
  "files": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/moulco/moul.git"
  },
  "author": "Sophearak Tha <hi@sophearak.me> (https://sophearak.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moulco/moul/issues"
  },
  "homepage": "https://moul.co/",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "bounds.js": "1.0.4",
    "husky": "4.2.3",
    "image-layout": "0.4.1",
    "parcel-bundler": "1.12.4",
    "photoswipe": "4.1.3",
    "prettier": "2.0.2",
    "pretty-quick": "2.0.1",
    "react": "16.13.0",
    "react-dom": "16.13.0"
  }
}
